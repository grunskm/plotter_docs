void setup() {
  Serial.begin(9600);

  servo.attach(9);
  raise_pen();
  delay(2000);

  pinMode(x_step, OUTPUT);
  pinMode(x_dir, OUTPUT);
  pinMode(y_step, OUTPUT);
  pinMode(y_dir, OUTPUT);

  pinMode(buttonPin, INPUT);

  //perimeter();

  int rows = 80;
  int cols = 64;
  
  int k = 0;
  int last_point = 0;

  int space = 400;
  raise_pen();
  
  for( int i=0; i<rows; i++){
    if(printAborted==false){
      moveToPoint(0,i*space);
      
      if(pgm_read_byte(data+k)==1){
        lower_pen();
        last_point = 1;
      }else{
        raise_pen();
        last_point = 0;
      }
    
    for( int e=0; e<cols; e++){
      if(printAborted==false){
        
        int data_point = pgm_read_byte(data+k);
  
        if(data_point != last_point&& pen_engaged== true){
         moveToPoint((e-1)*space,i*space);
         toggle_pen();
         last_point = data_point;
        }else if(data_point != last_point){
         moveToPoint(e*space,i*space);
         toggle_pen();
         last_point = data_point;
        }
        k++;
      }else{break;}
    }
    moveToPoint((cols-1)*space,i*space);
    raise_pen();
    }else{break;}
  }

  returnToHome();
  
  k = 0;
  last_point = 0;

  for( int i=0; i<cols; i++){
    if(printAborted==false){
      moveToPoint(i*space, 0);
      if(pgm_read_byte(data+i)==1){
        lower_pen();
        last_point = 1;
      }else{
        raise_pen();
        last_point = 0;
      }
    
    for( int e=0; e<rows; e++){
      if(printAborted==false){
        k = (e*cols)+i;
        int data_point = pgm_read_byte(data+k);
  
        if(data_point != last_point && pen_engaged== true){
         moveToPoint(i*space,(e-1)*space);
         toggle_pen();
         last_point = data_point;
        }else if(data_point != last_point){
         moveToPoint(i*space,e*space);
         toggle_pen();
         last_point = data_point; 
        }
        
      }else{break;}
    }
    moveToPoint(i*space,(rows-1)*space);
    raise_pen();
    }else{break;}
  }
 

  returnToHome();
}


